@prefix fuseki:  <http://jena.apache.org/fuseki#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tdb:     <http://jena.hpl.hp.com/2008/tdb#> .
@prefix ja:      <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix :        <#> .

<#service1> rdf:type fuseki:Service ;
    fuseki:name                       "douban_kgqa" ;     
    fuseki:serviceQuery               "sparql", "query" ; 
    fuseki:serviceReadGraphStore      "get" ;
    fuseki:dataset                   <#dataset> ;
    .

<#dataset> rdf:type ja:RDFDataset ;
	ja:defaultGraph <#modelInf> ;
	.
	
<#modelInf> rdf:type ja:InfModel ;
	ja:MemoryModel  <#tdbGraph> ;
	# 本体文件路径
	ja:content[ja:externalContent </Users/zhenhaiwang/GitHub/DouBan-KGQA/module/jena/apache-jena-fuseki-3.10.0.2/run/databases/douban_kgqa_ontology.ttl> ] ;
	# 启用OWL推理机
	ja:reasoner [ ja:reasonerURL <http://jena.hpl.hp.com/2003/OWLFBRuleReasoner>]
	.
	
<#tdbGraph> rdf:type tdb:GraphTDB ;
    tdb:location "/Users/zhenhaiwang/GitHub/DouBan-KGQA/data/tdb" ;
    tdb:unionDefaultGraph true ; 
	.
